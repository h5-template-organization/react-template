{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 5 versions", "not ie <= 10", "iOS >= 8", "Android >= 4.1"]
      }
    }],
    "react",
    "stage-2"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-runtime",
    ["import", {
      "libraryName": "antd-mobile",
      "style": true
    }]
  ]
}
